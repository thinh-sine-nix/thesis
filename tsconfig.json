{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es5",
        "declaration": true,
        "allowJs": true,
        "strictNullChecks": false,
        "noImplicitAny": false,
        "strict": false,
        "noEmit": true,
        "noEmitOnError": false,
        "strictFunctionTypes": false,
        "noImplicitThis": false,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "sourceMap": false,
        "outDir": "./dist",
        // "baseUrl": ".",
        "rootDir": ".",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "./src/**/*.ts",
    ],
    "exclude": [
        "node_modules",
    ]
}
